{
  "opts": {
    "template": "node_modules/zoo-jsdoc",
    "encoding": "utf8",
    "destination": "./doc/",
    "recurse": true,
    "private": false
  },
  "source": {
    "include": ["lib"]
  },
  "tags": {
    "allowUnknownTags" : true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles" : true
    },
    "applicationName": "PostgreSQL Test Utilities",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": ["examples"]
  }
}