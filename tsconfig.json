  "jsdoc": "jsdoc/out",
  "compilerOptions": {
    "strict": true,
    "module": "commonJS",
    "outDir": "lib",
    "sourceMap": true,
    "declaration": true,
    "target": "esnext",
    "lib": ["es2015", "es2016", "es2017"]
